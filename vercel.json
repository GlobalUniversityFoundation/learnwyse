{
  "headers": [
    {
      "source": "/(.*\\.mp4)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Type",
          "value": "video/mp4"
        },
        {
          "key": "Accept-Ranges",
          "value": "bytes"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Content-Length",
          "value": ""
        }
      ]
    }
  ],
  "functions": {
    "**": {
      "maxDuration": 300
    }
  }
}

